{
  "name": "storybook-sample",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "postbuild": "rm dist/mockServiceWorker.js",
    "gen:api-schema": "openapi-typescript openapi.yaml -t -o src/schema.d.ts --properties-required-by-default",
    "postgen:all": "pnpm type-check",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.ts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:docs": "storybook build --docs",
    "storybook:test": "test-storybook",
    "storybook:screenshot": "SCREEN_SHOT=true test-storybook",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "vue": "^3.5.7",
    "vue-router": "^4.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@playwright/test": "^1.47.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@storybook/addon-essentials": "^8.3.2",
    "@storybook/addon-interactions": "^8.3.2",
    "@storybook/addon-links": "^8.3.2",
    "@storybook/addon-mdx-gfm": "^8.3.2",
    "@storybook/addon-onboarding": "^8.3.2",
    "@storybook/blocks": "^8.3.2",
    "@storybook/test": "^8.3.2",
    "@storybook/test-runner": "^0.17.0",
    "@storybook/vue3": "^8.3.2",
    "@storybook/vue3-vite": "^8.3.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.1",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.28.0",
    "happy-dom": "^14.12.3",
    "jsdom": "^24.1.3",
    "msw": "^2.4.9",
    "msw-storybook-addon": "^2.0.3",
    "npm-run-all2": "^6.2.3",
    "openapi-fetch": "^0.9.8",
    "openapi-msw": "^0.6.1",
    "openapi-typescript": "^7.4.1",
    "playwright": "^1.47.2",
    "prettier": "^3.3.3",
    "storybook": "^8.3.2",
    "storycap-testrun": "^0.0.2",
    "typescript": "~5.4.5",
    "vite": "^5.4.7",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.1.6"
  },
  "volta": {
    "node": "20.15.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "packageManager": "pnpm@9.2.0+sha512.98a80fd11c2e7096747762304106432b3ddc67dcf54b5a8c01c93f68a2cd5e05e6821849522a06fb76284d41a2660d5e334f2ee3bbf29183bf2e739b1dafa771"
}
